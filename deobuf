local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

if character:WaitForChild("Humanoid").RigType == Enum.HumanoidRigType.R15 then

    loadstring(game:HttpGet("https://raw.githubusercontent.com/hogamg3/r15-animation/refs/heads/main/deobuf"))()
else
    loadstring(game:HttpGet("https://raw.githubusercontent.com/hogamg3/r6-anim/refs/heads/main/r6"))();
end
